

public class ThreeConsecutiveOdds {

	public static void main(String[] args) {
		
		int[] arr= {1,2,3,5,7};
		
		int count=0;
		for(int i=0;i<arr.length;i++) {
			if(arr[i]%2!=0) {
				count++;
				
			} 
			else {
				count=0;
			}
			if(count==3) {
				System.out.println(true);
			}
		}
		if(count!=3) {
			System.out.println(false);
		}
			
			
			
		
		
		
	}

}
